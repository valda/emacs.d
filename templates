;;; ~/.emacs.d/templates

emacs-lisp-mode

(up "(use-package " p n> ":straight t" n> ":defer t" n> ":config" n> p ")")

web-mode html-mode css-mode

(r "<% " p " -%>")
(rr "<%= " p " %>")
(pe "<?php echo " p " ?>")

org-mode

(ai      & "#+begin_ai" n> r> n> "#+end_ai")
(image   & "#+begin_ai :image" n> r> n> "#+end_ai")

